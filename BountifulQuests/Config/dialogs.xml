<config>
  <!-- Prune any quest entries beyond the 15 this tweak supports. Some quest mods,
       such as MoreQuests add up to 60. -->
  <conditional>
    <if cond="mod_loaded('AGF-V2-HUDPlus-1Main')">
      <remove
        xpath="//dialogs/dialog[@id='trader']/statement[starts-with(@id,'currentjobs')]/quest_entry[@listindex >= 15]" />
    </if>
  </conditional>
</config>
